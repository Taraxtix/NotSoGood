// ELSE-LESS IF
if 1 1 = do     //
    1 print     // 1
end             //

if 0 1 = do     //
    0 print     //
end             //

// IF-ELSE
if 1 1 = do     //
    1 print     //
else            // 1
    0 print     //
end             //

if 0 1 = do     //
    1 print     //
else            // 0
    0 print     //
end             //

// WHILE
10 while dup 0 >= do    // 10
    dup print           // 9
    1 -                 // 8
end drop                // 7
                        // 6
                        // 5
                        // 4
                        // 3
                        // 2
                        // 1
                        // 0